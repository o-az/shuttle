{
  "deno.enable": true,
  "deno.path": "/opt/homebrew/bin/deno",
  "deno.unstable": true,
  "deno.lint": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "files.associations": {
    "deno.json": "jsonc",
    ".env.ts": "typescript"
  }
}
